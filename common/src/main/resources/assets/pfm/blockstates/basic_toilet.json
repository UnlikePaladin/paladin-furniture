        {
            "when": { "AND": [
                { "facing": "west" },
                { "toilet": "flushing" }
            ]},
            "apply": { "model": "pfm:block/basic_toilet/basic_toilet_flowing_water"}
        },
        {
            "when": { "AND": [
                { "facing": "north" },
                { "toilet": "clean" }
            ]},
            "apply": { "model": "pfm:block/basic_toilet/basic_toilet_still_water", "y":90}
        },
        {
            "when": { "AND": [
                { "facing": "south" },
                { "toilet": "clean" }
            ]},
            "apply": { "model": "pfm:block/basic_toilet/basic_toilet_still_water", "y":270}
        },
        {
            "when": { "AND": [
                { "facing": "east" },
                { "toilet": "clean" }
            ]},
            "apply": { "model": "pfm:block/basic_toilet/basic_toilet_still_water", "y":180}
        },
        {
            "when": { "AND": [
                { "facing": "west" },
                { "toilet": "clean" }
            ]},
            "apply": { "model": "pfm:block/basic_toilet/basic_toilet_still_water"}
        },
        {
            "when": { "AND": [
                { "facing": "north" },
                { "toilet": "empty" }
            ]},
            "apply": { "model": "pfm:block/basic_toilet/basic_toilet_empty", "y":90}
